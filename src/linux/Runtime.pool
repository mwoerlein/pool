namespace linux;

class Runtime { //extends sys::core::Runtime {
    
    // methods
    global [Runtime] buildRuntime() {
        // Write the string to stdout:
        __pasm__("call strLen; int 0x80", {"%eax": 4, "%ebx": 1, "%ecx": "bootstrap runtime\n"});
        return null;
    }
}
