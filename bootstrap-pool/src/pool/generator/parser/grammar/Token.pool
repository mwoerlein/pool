namespace pool::generator::parser::grammar;

use pool::generator::Symbol;

use sys::core::String;
use sys::core::anycollection::AnyIterator;
use sys::core::anycollection::AnyList;

class Token extends sys::core::Object {
    
    Symbol symbol;
    String definition;
    
    [] __init() { definition = this.createOwn(String:CLASSNAME); }
    
    [Token] setSymbol(Symbol s) { this.symbol = s; return this; }
    [Symbol] getSymbol() { return symbol; }
    
    [Token] setDefinition(cstring def) { this.definition.assignCString(def); return this; }
    [String] getDefinition() { return this.definition; }
    
}
